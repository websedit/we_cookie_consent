            <trans-unit id="tx_wecookieconsent_domain_model_service.google_consent_mode.description.analytics_storage">
                <target>Ermöglicht die Speicherung durch den Browser (z. B. Cookies) im Zusammenhang mit derAnalyse, z. B. der Besuchsdauer.</target>
            </trans-unit>
            <trans-unit id="tx_wecookieconsent_domain_model_service.google_consent_mode.description.ad_user_data">
